{{#each this.animals}}
    Animal = {{this.animalName}}
    Animal = {{this.animalNames}}
    {{#each this.foods}}
        Name = {{this.name}}
        Type Name = {{this.type.name}}
        {{#with this.type}}
            Type Name = {{this/name}}
            Food Name = {{../this.name}}
            Food Name = {{../name}}
            No Name = {{../../name}}
            Food Name = {{../this.this.this.name}}
            Animal Name = {{../animalName}}
            Animal Name = {{../../animalName}}
            No Name = {{this.animalName}}
            Animal Name = {{this../animalName}} {{!-- wtf --}}
            Type Name = {{name}}
        {{/with}}
    {{/each}}
{{/each}}